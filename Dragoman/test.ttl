@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix eablock: <https://github.com/SDM-TIB/EABlock> .
@prefix sdmkgc: <https://github.com/SDM-TIB/KGC/vocab/> .
@prefix ex: <https://example.com/> .
	
<drug>
	rml:logicalSource [ rml:source "/mnt/e/GitHub/ESWC-Demo/SDM-RDFizer/drug.csv";
						rml:referenceFormulation ql:CSV 
						];
    rr:subjectMap <function1> ;
    rr:termType rr:IRI;

    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:objectMap [ 
            rr:template "https://github.com/SDM-TIB/KGC/vocab/Drug"; 
        ]
    ].

<drug_disorder>
	rml:logicalSource [ rml:source "/mnt/e/GitHub/ESWC-Demo/SDM-RDFizer/drug.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "https://github.com/SDM-TIB/KGC/entity/{DrugBankID}";
		rr:class sdmkgc:DrugDisorderInteraction
	];
	rr:predicateObjectMap [
		rr:predicate sdmkgc:interactor1;
		rr:objectMap <function1> ;
	].

##########################################################################################
######################################## Functions #######################################
##########################################################################################

<function1>
    a fnml:FunctionTermMap;
    rr:termType rr:IRI;

    fnml:functionValue [
        rml:logicalSource [ rml:source "/mnt/e/GitHub/ESWC-Demo/SDM-RDFizer/drug.csv";
        rml:referenceFormulation ql:CSV                   
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant ex:tolower 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value;
            rr:objectMap [ 
                rr:template "https://github.com/SDM-TIB/KGC/entity/{DrugName}"; 
            ]
        ];   
    ]. 


