<file:///mnt/e/GitHub/ESWC-Demo/Dragoman/drug_disorder>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/mnt/e/GitHub/ESWC-Demo/Dragoman/output/test_PROJECT1.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "https://github.com/SDM-TIB/KGC/entity/{DrugBankID}";
        rr:class sdmkgc:DrugDisorderInteraction
    ];
    rr:predicateObjectMap [
        rr:predicate sdmkgc:interactor1;
        rr:objectMap [
        rr:parentTriplesMap <tolower_output1>;
        rr:joinCondition [
            rr:child "DrugName";
            rr:parent "DrugName";
            ];
        ];
    ].

<file:///mnt/e/GitHub/ESWC-Demo/Dragoman/drug>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/mnt/e/GitHub/ESWC-Demo/Dragoman/output/test_PROJECT2.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rml:reference "tolower_ub1bL51C24";
        rr:termType rr:IRI
    ];
    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:objectMap [
        rr:template  "https://github.com/SDM-TIB/KGC/vocab/Drug"
        ]
    ].

<tolower_output1>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/mnt/e/GitHub/ESWC-Demo/Dragoman/output/test_OUTPUT1.csv";
                rml:referenceFormulation ql:CSV
            ];
    rr:subjectMap [
        rml:reference "tolower_output1";
        rr:termType rr:IRI
    ].

