@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix eablock: <https://github.com/SDM-TIB/EABlock> .
@prefix sdmkgc: <https://github.com/SDM-TIB/KGC/vocab/> .

<disorder>
	rml:logicalSource [ rml:source "/data/disorder.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap <disorder_UMLSFunction> ;
    rr:termType rr:IRI;

    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:objectMap [ 
            rr:template "https://github.com/SDM-TIB/KGC/vocab/Disorder"; 
        ]
    ].	

<drug>
	rml:logicalSource [ rml:source "/data/drug.csv";
						rml:referenceFormulation ql:CSV 
						];
    rr:subjectMap <drug_FalconUMLS-Function> ;
    rr:termType rr:IRI;

    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:objectMap [ 
            rr:template "https://github.com/SDM-TIB/KGC/vocab/Drug"; 
        ]
    ].


<drug_disorder>
	rml:logicalSource [ rml:source "/data/drug_disorder.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "https://github.com/SDM-TIB/KGC/entity/{DrugBankID}_isPrescribedFor_{name}";
		rr:class sdmkgc:DrugDisorderInteraction
	];
	rr:predicateObjectMap [
		rr:predicate sdmkgc:interactor1;
		rr:objectMap <drug_FalconUMLS-Function> ;
	];
	rr:predicateObjectMap [
		rr:predicate sdmkgc:interactor2;
		rr:objectMap <disorder_UMLSFunction> ;
	].

##########################################################################################
######################################## Functions #######################################
##########################################################################################

####### Disorder ######
<disorder_FalconUMLS-Function>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/data/disorder.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant sdmkgc:falcon_UMLS_CUI_function 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate sdmkgc:value;
            rr:objectMap <disorder_replace-Function>
        ];    
    ].

<disorder_replace-Function>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/data/disorder.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant sdmkgc:replace 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate sdmkgc:value1;
            rr:objectMap [ 
                rml:reference "name" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate sdmkgc:value2;
            rr:objectMap [ 
                rr:constant "_" 
            ]
        ];     
    ].

####### Drug ######

<drug_FalconUMLS-Function>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/data/drug.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant sdmkgc:falcon_UMLS_CUI_function 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate sdmkgc:value;
            rr:objectMap [ 
                rr:template "https://github.com/SDM-TIB/KGC/entity/{DrugName}"; 
            ] 
        ];    
    ].
