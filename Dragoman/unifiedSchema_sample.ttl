@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .

#################################################################################################################################################
#################################################################################################################################################

<https://github.com/SDM-TIB/ESWC-Demo/tree/master/Dragoman>	rdf:type owl:Ontology ;
	                                   dc:contributor "Samaneh Jozashoori" ;
	                                   dc:creator "SDM-TIB" ;
	                                   dc:title "SDM-KGC Unified Schema" ;
	                                   owl:versionInfo "v1.0" .

#################################################################################################################################################
#################################################################################################################################################
#################################################################################################################################################
###############################################################   Classes   #####################################################################
#################################################################################################################################################
#################################################################################################################################################
#################################################################################################################################################

sdmkgc:Drug	rdf:type	owl:Class;
				rdfs:label	"Drug";
				rdfs:subClassOf	sdmkgc:Concept;
				rdfs:comment "A substance used as a medication or in the preparation of medication.";
				dcterms:modified "2022-05-12"^^xsd:date;
				dcterms:creator <https://www.tib.eu/en/research-development/scientific-data-management/staff/samaneh-jozashoori/>.

sdmkgc:Disorder	rdf:type	owl:Class;
					rdfs:label	"Disorder";
					rdfs:subClassOf	sdmkgc:Concept;
					rdfs:comment "A particular abnormal condition that negatively affects the structure or function of all or part of an organism, and that is not due to any immediate external injury.";
					dcterms:modified "2022-05-12"^^xsd:date;
					dcterms:creator <https://www.tib.eu/en/research-development/scientific-data-management/staff/samaneh-jozashoori/>.

sdmkgc:Interaction	rdf:type owl:Class;
					rdfs:label	"Interaction";
					rdfs:comment "Interaction between drugs and disorder, enzyme, target.";
					dcterms:modified "2022-05-12"^^xsd:date;
					dcterms:creator <https://www.tib.eu/en/research-development/scientific-data-management/staff/samaneh-jozashoori/>.

sdmkgc:DrugDisorderInteraction	rdf:type	owl:Class;
								rdfs:label	"DrugDisorderInteraction";
								rdfs:subClassOf	sdmkgc:Interaction;
								rdfs:comment "A drug-induced disorder.";
								dcterms:modified "2022-05-12"^^xsd:date;
								dcterms:creator <https://www.tib.eu/en/research-development/scientific-data-management/staff/maria-isabel-castellanos/>.

sdmkgc:interactor1	rdf:type owl:ObjectProperty;
					rdfs:label "interactor1";
					rdfs:domain sdmkgc:Interaction;
				  	rdfs:range sdmkgc:Drug;
					rdfs:comment "Interactor 1";
					dcterms:modified "2022-05-12"^^xsd:date;
					dcterms:creator <https://www.tib.eu/en/research-development/scientific-data-management/staff/samaneh-jozashoori/>.

sdmkgc:interactor2	rdf:type owl:ObjectProperty;
					rdfs:label "interactor2";
					rdfs:domain sdmkgc:Interaction;
				  	rdfs:range sdmkgc:Concept;
					rdfs:comment "Interactor 2";
					dcterms:modified "2022-05-12"^^xsd:date;
					dcterms:creator <https://www.tib.eu/en/research-development/scientific-data-management/staff/samaneh-jozashoori/>.

